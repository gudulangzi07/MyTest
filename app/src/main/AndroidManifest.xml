<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mytest">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activity.PolygonsActivity"/>
        <activity android:name=".activity.GameActivity"/>
        <activity android:name=".fivegame.FiveGameActivity"/>
        <activity android:name=".activity.MyNineActivity"/>
        <activity android:name=".activity.MyTimeActivity"/>
        <activity android:name=".activity.MoveActivity"/>
        <activity android:name=".activity.TimeLineActivity"/>
        <activity android:name=".activity.EventPassActivity"/>
        <activity android:name=".activity.ChangeSurfaceViewActivity"/>
        <activity android:name=".activity.TwoSurfaceViewActivity"/>
        <activity android:name=".mvvm.activity.MVVMActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"/>
    </application>

</manifest>